{
  "name": "url-shortener-server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "pretest": "cross-env DATABASE_URL=file:./prisma/test.db prisma db push --force-reset --skip-generate",
    "test": "cross-env NODE_ENV=test DATABASE_URL=file:./prisma/test.db vitest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.15",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "prisma": "^5.13.0",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.4.0"
  }
}
